<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
           <record id="nozzle_report_wizard_view_form" model="ir.ui.view">
            <field name="name">nozzle.report.wizard.form</field>
            <field name="model">nozzle.report.wizard</field>
            <field name="arch" type="xml">
                <form string="Daily Summary Of Fuel Pump">
                    <group>
                        <group >
                            <field name="start_at"/>
                            <field name="branch_id"/>
                        </group>
                    </group>
                    <footer>
                        <button name="print_pdf_report" type="object" string="Print XLS" class="oe_highlight"/>
                        <button special="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>




         <record id="action_product_nozzle_report" model="ir.actions.act_window">
            <field name="name">Daily Summary Of Fuel Pump</field>
            <field name="res_model">nozzle.report.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

          <menuitem id="menu_nozzle_report"
            action="action_product_nozzle_report"
            parent="dxl_gas_station.menu_sub_reports"
            sequence="20"/>
    </data>
</odoo>
